{
  "title": "Form Builder - Shaman Factory",
  "shaman": {
    "query": [{
      "name": "allForms",
      "path": "forms",
      "args": ["*"],
      "sort": { "key": "createdDate", "descending": true }
    }],
    "bundles": [
      {
        "name": "index.styles.bundle",
        "type": "css",
        "files": [
          "styles/bootstrap.css",
          "styles/card.css",
          "styles/main.css"
        ]
      },
      {
        "name": "index.scripts.bundle",
        "type": "js",
        "files": [
          "scripts/index.js"
        ]
      }
    ]
  }
}