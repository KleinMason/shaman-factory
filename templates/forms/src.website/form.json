{
  "shaman": {
    "dynamic": {
      "path": "form/",
      "name": "route"
    },
    "query": [{
      "path": "forms",
      "dynamic": true,
      "args": ["*"]
    }],
    "bundles": [
      {
        "name": "form.styles.bundle",
        "type": "css",
        "files": [
          "styles/bootstrap.css",
          "styles/main.css",
          "styles/panel.css"
        ]
      },
      {
        "name": "form.scripts.bundle",
        "type": "js",
        "files": [
          "scripts/jquery.js",
          "scripts/popper.js",
          "scripts/bootstrap.js",
          "scripts/index.js",
          "scripts/form.js"
        ]
      }
    ]
  }
}